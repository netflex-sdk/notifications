{
    "name": "netflex/notifications",
    "description": "Netflex Notifications Mail Driver for Laravel",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "illuminate/mail": "^8.0",
        "illuminate/support": "^8.0",
        "mustache/mustache": "^2.13",
        "netflex/api": "^4.0.4",
        "netflex/foundation": "^4.0.4",
        "netflex/renderer": "^4.0.4",
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Notifications\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netflex\\Notifications\\Providers\\NotifcationsServiceProvider"
            ]
        }
    }
}

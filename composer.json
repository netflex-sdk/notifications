{
    "name": "netflex/notifications",
    "description": "Netflex Notifications Mail Driver for Laravel",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "illuminate/bus": "^12.0",
        "illuminate/mail": "^12.0",
        "illuminate/notifications": "^12.0",
        "illuminate/support": "^12.0",
        "mustache/mustache": "^2.14.1",
        "netflex/api": "^6.0",
        "netflex/foundation": "^6.0",
        "netflex/renderer": "^6.0",
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Notifications\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Notifications\\Providers\\NotificationsServiceProvider"
            ]
        }
    }
}
